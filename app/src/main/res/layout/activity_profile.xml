				<TextView
					android:text="@string/weight_units"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:textAppearance="?android:attr/textAppearanceMedium"
					android:id="@+id/weight_unit_target"
					android:textSize="16sp"
					android:layout_column="2"
					android:textStyle="bold"
					android:layout_gravity="center"/>
			</TableRow>
			<TableRow
				android:layout_width="match_parent"
				android:layout_height="18sp"
				android:layout_gravity="left|bottom">
				<TextView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="@string/user_bai_static"
					android:id="@+id/user_bai_static"
					android:layout_column="0"
					android:textStyle="normal|bold"
					android:textSize="14sp"
					android:minWidth="140sp"
					android:maxWidth="80sp"
					android:textAlignment="gravity"
					android:typeface="sans"
					android:textIsSelectable="false"
					android:layout_marginLeft="8sp"
					android:gravity="left"
					android:visibility="visible"
					android:layout_gravity="center_vertical"/>
				<EditText
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:text="@string/user_bai_value"
					android:id="@+id/user_bai_value"
					android:layout_column="1"
					android:layout_gravity="left"
					android:inputType="number|text"
					android:textSize="14sp"
					android:textStyle="normal|bold"
					android:gravity="left|start"
					android:editable="true"
					android:enabled="true"
					android:focusable="true"
					android:minWidth="20sp"
					android:maxWidth="40sp"
					android:typeface="sans"
					android:width="80sp"
					android:textIsSelectable="true"
					android:clickable="true"
					android:hint="@string/user_bai_value_hint"
