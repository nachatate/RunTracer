<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
            android:id="@+id/ScrollView01"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:minWidth="140sp">
	<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	              android:orientation="vertical" android:layout_width="fill_parent"
	              android:layout_height="wrap_content"
	              android:weightSum="1">

		<TableLayout
			android:id="@+id/new_user_form"
			android:layout_width="wrap_content"
			android:layout_height="0dp"
			android:layout_gravity="center_horizontal"
			android:orientation="vertical"
			android:layout_marginTop="20sp"
			android:layout_weight="0.07">
			<TableRow
				android:layout_width="80sp"
				android:layout_height="20sp"
				android:layout_gravity="left|bottom">
				<TextView
					android:layout_width="100sp"
					android:layout_height="28sp"
					android:id="@+id/title_activity_new_user"
					android:text="@string/space_static"
					android:textSize="28sp"
					android:layout_column="1"
					android:layout_gravity="center_vertical"
					android:textAlignment="gravity"
					android:singleLine="false"
					android:typeface="sans"
					android:layout_marginLeft="8sp"
					android:layout_marginBottom="12sp"
					android:textStyle="normal|bold"
					android:minWidth="60sp"
					android:maxWidth="80sp" />
			</TableRow>
			<TableRow
				android:layout_width="80sp"
				android:layout_height="40sp"
				android:layout_gravity="left|bottom">
				<TextView
					android:layout_width="match_parent"
					android:layout_height="match_parent"
					android:id="@+id/full_name_static"
					android:text="@string/full_name_static"
					android:textSize="14sp"
					android:layout_column="0"
					android:textAlignment="gravity"
					android:singleLine="false"
					android:typeface="sans"
					android:layout_marginLeft="8sp"
					android:textStyle="normal|bold"
					android:minWidth="120sp"
					android:maxWidth="120sp"
					android:visibility="visible"
					android:layout_gravity="center_vertical"/>
				<AutoCompleteTextView
					android:id="@+id/full_name"
					android:layout_width="match_parent"
					android:layout_height="match_parent"
					android:inputType="textCapWords|textPersonName|textAutoComplete"
					android:hint="@string/full_name_hint"
					android:text="@string/full_name"
					android:layout_column="1"
					android:maxLines="1"
					android:singleLine="true"
					android:textSize="14sp"
					android:layout_gravity="left"
					android:visibility="visible"
					android:editable="true"
					android:enabled="true"
					android:focusable="true"
					android:focusableInTouchMode="true"
					android:textStyle="bold"
					android:minWidth="190sp"/>
			</TableRow>
			<TableRow
				android:layout_width="80sp"
				android:layout_height="40sp"
				android:layout_gravity="left|bottom">
				<TextView
					android:layout_width="match_parent"
					android:layout_height="match_parent"
					android:text="@string/email_static"
					android:id="@+id/email_static"
					android:layout_column="0"
					android:textStyle="normal|bold"
					android:textSize="14sp"
					android:layout_marginLeft="8sp"
					android:textAlignment="gravity"
					android:typeface="sans"
					android:visibility="visible"
					android:layout_gravity="center_vertical"
					android:minWidth="120sp"/>
				<EditText
					android:layout_width="match_parent"
					android:layout_height="match_parent"
					android:id="@+id/email"
					android:layout_column="1"
					android:textSize="14sp"
					android:enabled="true"
					android:focusable="true"
					android:minWidth="190sp"
					android:maxWidth="80sp"
					android:textIsSelectable="false"
					android:clickable="false"
					android:hint="@string/email_hint"
					android:text="@string/email"
					android:editable="true"
					android:focusableInTouchMode="true"
					android:autoLink="email"
					android:inputType="textEmailAddress|textAutoComplete"
					android:autoText="false"
					android:visibility="visible"
					android:textStyle="bold"/>
			</TableRow>
			<TableRow
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:layout_gravity="left|bottom"
				android:visibility="visible">
				<TextView
					android:layout_width="match_parent"
					android:layout_height="match_parent"
					android:id="@+id/user_gender_static"
					android:layout_column="0"
					android:textSize="14sp"
					android:enabled="true"
					android:focusable="true"
					android:minWidth="120sp"
					android:maxWidth="80sp"
					android:textIsSelectable="false"
					android:clickable="true"
					android:hint="@string/user_gender_hint"
					android:text="@string/user_gender_static"
					android:editable="true"
					android:focusableInTouchMode="true"
					android:autoLink="email"
					android:inputType="textEmailAddress|textAutoComplete"
					android:autoText="false"
					android:visibility="visible"
					android:layout_marginLeft="8sp"
					android:textStyle="bold"
					android:layout_gravity="center_vertical"/>

				<Switch
					android:layout_width="match_parent"
					android:layout_height="match_parent"
					android:id="@+id/user_gender_value"
					android:layout_column="1"
					android:textOn="Female"
					android:textOff="Male"
					android:hint="@string/user_gender_hint"
					android:textSize="14sp"
					android:minWidth="40sp"
					android:maxWidth="40sp"
					android:checked="true"
					android:enabled="true"
					android:focusable="true"
					android:focusableInTouchMode="true"
					android:textStyle="bold"
					android:clickable="true"/>
			</TableRow>



























			<TableRow
